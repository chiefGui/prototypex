a.back-link {
	text-decoration: none;
	color: #000;
}

a.course-title {
	background-color: #01528a;
	text-decoration: none;
	color: #fff;
	margin-top: 10px;
	display: block;
	padding: 5px;
	clear: both;
}

h1.main-description {
	font-size: 20px !important;
	margin-bottom: 10px;
}

ul.info-list {
	width: 499px;
	background-color: #ececec !important;
}

ul.info-list li {
	float: left;
	padding: 6px 15px 6px 10px;
	font-size: 13px;
}

ul.facebook-actions {
	background-color: #f4f4f4;
}

ul.facebook-actions li {
	float: left;
	font-size: 13px;
	padding: 6px 15px 6px 22px;
}

ul.facebook-actions li a {
	font-weight: bold;
	color: #797979;
}

ul.facebook-actions li:first-child {
}

ul.facebook-actions li a {
	text-decoration: none;
	color: #797979;
}

div.objectives {
	margin: 20px 0;
}

p.main-text {
	width: 600px;
	margin-left: 15px;
}

div.post-menu {
	float: left;
	width: 790px;
	clear: both;
}